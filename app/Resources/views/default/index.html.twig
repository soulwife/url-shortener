{% extends 'base.html.twig' %}

{% block main %}
<div id="wrapper">
    <div class="container">
        <div class="row" id="welcome">
            <h1><span>Make your url shorter</span> </h1>
        </div>
        <div class="row">
            <div class="col-lg">
                {{ form_start(form) }}
                {{ form_errors(form) }}

                {{ form_row(form.initialUrl) }}
                {{ form_row(form.expiredAt) }}
                {{ form_end(form) }}
            </div>
        </div>
    </div>
</div>
{% endblock %}